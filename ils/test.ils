integer: int = 1+2*(6+0);
array: [int] = {1+1,2,3,4,5};

fun sum(a:int, b:int) -> int
{
	return a + b;
}

fun test()
{
	@println("Hello world!");
}

fun main() -> int
{
	test();
	x: int = sum(1, sum(3,2));
	@println(x);
	x = integer;
	@println(x);
	x = array[2];
	@println(x);

	while(integer > 0)
	{
		if(integer % 2 == 0)
		{
			@println(integer);
		}
		integer = integer - 1;
	}

	return 0;
}