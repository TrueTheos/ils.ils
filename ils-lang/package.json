{
  "name": "ils-lang",
  "displayName": "ILS Lang",
  "description": "Syntax highlighting for ILS lang.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ils",
      "aliases": ["ILS", "ils"],
      "extensions": [".ils"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ils",
      "scopeName": "source.ils",
      "path": "./syntaxes/ils.tmLanguage.json"
    }]
  }
}
